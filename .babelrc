{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "ignore": ["node_modules"],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", { "runtime": "automatic" }],
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "_": "./src/_shadcn"
                }
            }
        ]
    ]
}
